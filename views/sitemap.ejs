<?xml version="1.0" encoding="UTF-8"?>

<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
	<!-- Article Listings -->
	<url>
		<loc>https://snitchbcc.com</loc>
		<changefreq>daily</changefreq>
		<priority>1</priority>
	</url>
	<url>
		<loc>https://snitchbcc.com/advice</loc>
		<changefreq>daily</changefreq>
		<priority>1</priority>
	</url>
	<url>
		<loc>https://snitchbcc.com/local</loc>
		<changefreq>daily</changefreq>
		<priority>1</priority>
	</url>
	<url>
		<loc>https://snitchbcc.com/politics</loc>
		<changefreq>daily</changefreq>
		<priority>1</priority>
	</url>
	<url>
		<loc>https://snitchbcc.com/culture</loc>
		<changefreq>daily</changefreq>
		<priority>1</priority>
	</url>

	<!-- Meta -->
	<url>
		<loc>https://snitchbcc.com/about</loc>
		<changefreq>monthly</changefreq>
		<priority>0.8</priority>
	</url>

	<!-- Articles -->
	<% for (const article of articles) { %>
		<url>
			<loc>https://snitchbcc.com/article/<%- article.slug %></loc>
			<changefreq>monthly</changefreq>
			<priority>0.5</priority>
		</url>
	<% } %>
</urlset> 
